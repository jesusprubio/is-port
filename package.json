  "keywords": [
    "port",
    "valid",
    "check",
    "IANA"
  ],
  "bugs": {
    "url": "https://github.com/jesusprubio/is-port/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Jesus Rubio",
    "url": "http://jesusprubio.name"
  },
  "files": [
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jesusprubio/is-port/is-port.git"
  },
  "scripts": {
    "format": "prettier-tslint fix 'src/**/*.ts'",
    "lint": "tslint -p tsconfig.json",
    "precommit": "lint-staged",
    "deps": "depcheck --ignores=lint-staged,np,prettier-tslint,typescript,tslint,eslint",
    "audit": "audit-ci --moderate",
    "pretest": "npm run audit && npm run deps && npm run lint",
    "test": "tsc && nyc ava",
    "release": "np --contents dist"
  },
  "dependencies": {},
  "devDependencies": {
    "audit-ci": "^1.4.1",
    "ava": "^1.3.1",
    "depcheck": "^0.7.2",
    "eslint": "^5.3.0",
    "lint-staged": "^8.1.5",
    "np": "^4.0.2",
    "nyc": "^13.3.0",
    "prettier-tslint": "^0.4.2",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "engines": {
    "node": ">=10"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
